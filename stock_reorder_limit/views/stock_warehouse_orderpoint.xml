<?xml version="1.0" encoding="UTF-8"?>
<openerp><data>

    <record model="ir.ui.view" id="view_warehouse_orderpoint_form">
        <field name="name">stock.warehouse.orderpoint.form</field>
        <field name="model">stock.warehouse.orderpoint</field>
        <field
            name="inherit_id"
            ref="stock.view_warehouse_orderpoint_form"
            />
        <field name="arch" type="xml">
            <xpath expr="//sheet/group" position="inside">
                <group string="Product data">
                    <field name="limit_procurement_qty"/>
                    <field name="product_state"/>
                    <field name="product_active"/>
                    <field name="product_purchase_ok"/>
                </group>
            </xpath>
        </field>
    </record>

</data></openerp>
